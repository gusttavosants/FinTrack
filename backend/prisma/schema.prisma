// prisma/schema.prisma
datasource db {
  provider = "sqlite"
  
}

generator client {
  provider = "prisma-client-js"
}

// O que vocÃª deve escrever abaixo:
model User {
  id            String @id @default(uuid())
  name          String
  email         String @unique
  password_hash String
  created_at    DateTime @default(now())

  @@map("users") // Nome da tabela no banco
}